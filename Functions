      function playSound(title, id, volume, canBeLooped, archivable)
      local Audio = Instance.new("Sound")
      Audio.Name = title;
      Audio.SoundId = "http://www.roblox.com/asset/?id=" .. id;
      Audio.Volume = volume;
      Audio.Looped = canBeLooped;
      Audio.Archivable = archivable;
      Audio.Parent = game.Workspace
      end
