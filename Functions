function playSound(song)
    if song == "intro" then
    local Audio = Instance.new("Sound")
    Audio.Name = "Introduction";
    Audio.SoundId = "http://www.roblox.com/asset/?id=1570675466";
    Audio.Volume = 5;
    Audio.Looped = false;
    Audio.Archivable = false;
    Audio.Parent = game.Workspace;
    end
end
